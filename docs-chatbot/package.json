    "typescript": "5.1.6",
    "edgedb": "^1.3.4",
    "openai": "^4.0.1"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "20.4.8",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@edgedb/generate": "^0.3.3",
    "dotenv": "^16.3.1",
    "gpt-tokenizer": "^2.1.1",
    "tsx": "^3.12.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "postcss": "8.4.27",
